#include <iostream>
using namespace std;
int main()
{ float C,F;
    cout <<"enter your temperature"<<endl;
    cin >>F;
    C=(F-32)*5/9;
    cout <<C<<endl;
    return 0;
}


#include <iostream>
using namespace std;
int main ()
{ float C,F;
    cout<<"enter your T"<<endl;
    cin >>C;
    F=(C*9/5)+32;
    cout <<F;
}
